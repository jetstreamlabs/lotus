{
	"name": "jetstreamlabs/lotus",
	"description": "Adds Action Domain Responder pattern with InertiaJS and TailwindCSS to Laravel 8 and above.",
	"type": "package",
	"require": {
		"php": "^8.0",
		"doctrine/dbal": "^3.3",
		"inertiajs/inertia-laravel": "^0.5.4",
		"jetstreamlabs/zora": "^3.0",
		"laravel/framework": "^9.51",
		"laravel/sanctum": "^2.0",
		"scrivo/highlight.php": "v9.18.1.9",
		"symfony/css-selector": "^6.0",
		"symfony/dom-crawler": "^6.0",
		"tightenco/ziggy": "^1.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "secondman",
			"email": "secondman@jetstreamlabs.com"
		}
	],
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"files": [
			"src/Lotus/helpers.php"
		],
		"psr-4": {
			"Jetlabs\\Lotus\\": "src/Lotus/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Jetlabs\\Lotus\\Providers\\LotusServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"styleci/cli": "^1.2"
	}
}
